<form method="post" action="{{ path('app_produit_delete', {'id': produit.id}) }}" onsubmit="return confirm('{{ 'Voulez-vous supprimer le produit?'|trans }}');"  style="display: inline;">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ produit.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash"></i></button>
</form>
