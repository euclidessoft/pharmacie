<!DOCTYPE html>

<title>Mutuelles index</title>

{% block body %}
    <h1>Mutuelles index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Adresse</th>
                <th>Telephone</th>
                <th>Montant</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for mutuelle in mutuelles %}
            <tr>
                <td>{{ mutuelle.id }}</td>
                <td>{{ mutuelle.nom }}</td>
                <td>{{ mutuelle.adresse }}</td>
                <td>{{ mutuelle.telephone }}</td>
                <td>{{ mutuelle.montant }}</td>
                <td>
                    <a href="{{ path('app_mutuelles_show', {'id': mutuelle.id}) }}">show</a>
                    <a href="{{ path('app_mutuelles_edit', {'id': mutuelle.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_mutuelles_new') }}">Create new</a>
{% endblock %}
