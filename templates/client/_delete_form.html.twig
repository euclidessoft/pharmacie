<form method="post" action="{{ path('app_client_delete', {'id': client.id}) }}" onsubmit="return confirm('{{ 'Voulez-vous supprimer le client?'|trans }}');"  style="display: inline;">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.id) }}">
    <button class="btn btn-danger"><span class="fa fa-trash"></span></button>
</form>
