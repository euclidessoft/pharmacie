    //hebergement
    function focu(id) {
        $('#tampon').val(id);
    }

    //hebergement
    function nofocu(id) {
        $('#tampon').val('');
    }

    //fonction appuie sur le bouton entrer
    function entrer() {
        var reg = new RegExp("^[0-9]+$");
        var id = $('#tampon').val();
        var quantite = $('#text'+id).val();
        if (reg.test(quantite) && window.event.keyCode == 13) {
            ajouter_vente(id);

        } //else alert("{{'invalide'|trans }}");

    }

