                                    "      <div class=\"modal-footer\">\n" +
                                    "        <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\" id='annuler" + data[i]['id'] + "'>Close</button>\n" +
                                    "        <button type=\"button\"  id='sub" + data[i]['id'] + "' class=\"btn btn-primary\" onclick='ajouter(" + data[i]['id'] + ")'>{{ 'Ajouter'|trans }}</button>\n" +
                                    "      </div>\n" +
                                    "    </div>\n" +
                                    "  </div>\n" +
                                    "</div></td></tr>");
                        }

                    } else {
                        $('#produits').html("<tr><td colspan='3'>Aucun produit</td></tr>");
                    }
                },
                error: function () {
                    alert('{{ "La requête n'a pas abouti"|trans }}');

                }
            });
        }
    }
    /*****Fnc ajout produit*******/
    function ajouter(produit) {

        var quant = $("#text" + produit);
        // var peremp = new Date($("#peremp" + produit).val());
        if(quant.val() != ""){
            if (quant.val() > 0) {
                $("#sub"+produit).html("<i class='fa-solid fa-spinner fa-spin-pulse fa-lg'></i>");
                $.ajax({
                    type: "POST",
                    url: "{{ path('commande_produit_add') }}",
                    data: { produit : produit , quantite : quant.val() },
                    success: function (data) {
                        if (data['id'] == 'ok') {
                            $('#commander').append(" <tr id='tr" + produit + "'>\n" +
                                "                        <td>" + data['ref'] + "</td>\n" +
                                "                        <td>" + data['designation'] + "</td>\n" +
                                "                        <td class='text-center'>" + data['quantite'] + "</td>\n" +
                                "                        <td> <button class='btn btn-danger mr-2 mb-2'\n" +
                                "                                   title=\"{{ 'Supprimer le produit'|trans}}\" onclick='supprim(" + produit + ")'>\n" +
                                "                                    <i class='fa fa-trash'></i>\n" +
                                "                                </button></td></tr>");

                            $("#annuler" + produit).trigger('click');
                            quant.val("");
                            $("#sub"+produit).html("{{ 'Ajouter'|trans }}");
                            $("#btn"+produit).addClass('disabled');
                            $('#valider').removeClass('disabled');
                         } else {
                            $("#annuler" + produit).trigger('click');
                            quant.val("");
                            $("#sub"+produit).html("{{ 'Ajouter'|trans }}");
                            $("#btn"+produit).addClass('disabled');
                            alert(data['id'])
                        }
                    },
                    error: function () {
                        alert("{{ 'La requête n’a pas abouti'|trans }}");
                    }
                });
            } else {
                alert("{{'Veillez bien renseigner la quantité'|trans}}");
            }
        }
        else{
            $("#sub"+produit).html("{{ 'Ajouter'|trans }}");
            alert("{{ 'Veillez bien renseigner la quantite'|trans }}");
        }
    }
    function supprim(produit) {

        if (confirm("{{ 'Etes-vous sur de vouloir supprimer?'|trans }}")) {
            $.ajax({
                type: "POST",
                url: "{{ path('commande_produit_delete') }}",
                data: { produit :produit },
                success: function (data) {
                    if (data['id'] == 'ok') {
                        $("#tr" + produit ).remove();
                        $('#btn' + produit).removeClass('disabled');
                        $("#text" + produit).val('');// vider le champ quantite dans modal ajouter
                        $("#ajouter" + produit).show();// afficher le bouton ajouter
                        $("#ajouter" + produit).html("<i class=\"fa fa-plus\"></i> {{ 'Ajouter'|trans }}");// afficher le bouton ajouter
                        $("#total").html(data['total']);// total pour la facturation
                        $("#td"+produit).html("<input type='number' id='text"+ produit+"' class='form-control' onfocus='focu("+produit+")' onblur='nofocu()' style='width:100px'>");// total pour la facturation


                        if (data['nb'] == 0) {
                            $('#valider').addClass('disabled');
                        }
                    }
                },
                error: function () {
                    alert("{{ 'La requête n’a pas abouti'|trans }}");
                }
            });
        }
    }
